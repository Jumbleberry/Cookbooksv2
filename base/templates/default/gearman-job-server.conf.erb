[program:gearman-job-server]
user=gearman
command=/usr/sbin/gearmand -L <%= node["gearman"]["host"] %> --job-retries=<%= node["gearman"]["retries"] %>
autostart=false
autorestart=unexpected
stdout_logfile=/var/log/supervisor/%(program_name)s.log
stderr_logfile=/var/log/supervisor/%(program_name)s_err.log
exitcodes=0