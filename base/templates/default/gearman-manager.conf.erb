[program:gearman-manager]
command=/usr/local/bin/gearman-manager -c /etc/gearman-manager/config.ini -l /var/log/gearman-manager.log -u <%= node[:user] %> -e dev
autostart=false
autorestart=unexpected
stdout_logfile=/var/log/supervisor/%(program_name)s.log
stderr_logfile=/var/log/supervisor/%(program_name)s_err.log
exitcodes=0